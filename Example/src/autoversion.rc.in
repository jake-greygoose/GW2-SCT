#include <windows.h>
#include <winver.h>

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @DETARGET_VERSION_CSV@
 PRODUCTVERSION @DETARGET_VERSION_CSV@
 FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
 FILEFLAGS 0x0L
 FILEOS VOS_NT_WINDOWS32
 FILETYPE VFT_DLL
 FILESUBTYPE VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "FileDescription", "Detarget"
            VALUE "FileVersion", "@DETARGET_VERSION@"
            VALUE "InternalName", "Detarget"
            VALUE "OriginalFilename", "Detarget.dll"
            VALUE "ProductName", "Detarget"
            VALUE "ProductVersion", "@DETARGET_VERSION@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1252
    END
END
